# SOTA RAG Environment Configuration
# Copy this file to .env and fill in your values

# MongoDB Atlas Connection
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/sota_rag?retryWrites=true&w=majority

# Voyage AI API Key
VOYAGE_API_KEY=pa-xxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Anthropic Claude API Key
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Google Gemini API Key (optional, for alternative LLM)
GOOGLE_API_KEY=

# Application Settings
LOG_LEVEL=INFO
ENVIRONMENT=development

# Cache Settings (optional)
EMBEDDING_CACHE_SIZE=5000
QUERY_CACHE_TTL=300

# Rate Limiting (optional)
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60
